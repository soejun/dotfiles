{
  "$help": "https://aka.ms/terminal-documentation",
  "$schema": "https://aka.ms/terminal-profiles-schema",
  "actions": [
    {
      "command": {
        "action": "copy",
        "singleLine": false
      },
      "keys": "ctrl+c"
    },
    {
      "command": {
        "action": "resizePane",
        "direction": "up"
      },
      "keys": "alt+shift+k"
    },
    {
      "command": "paste",
      "keys": "ctrl+v"
    },
    {
      "command": {
        "action": "resizePane",
        "direction": "left"
      },
      "keys": "alt+shift+h"
    },
    {
      "command": "find",
      "keys": "ctrl+shift+f"
    },
    {
      "command": {
        "action": "splitPane",
        "split": "auto",
        "splitMode": "duplicate"
      },
      "keys": "ctrl+shift+enter"
    },
    {
      "command": {
        "action": "resizePane",
        "direction": "down"
      },
      "keys": "alt+shift+j"
    },
    {
      "command": {
        "action": "moveFocus",
        "direction": "previousInOrder"
      },
      "keys": "ctrl+shift+["
    },
    {
      "command": {
        "action": "moveFocus",
        "direction": "nextInOrder"
      },
      "keys": "ctrl+shift+]"
    },
    {
      "command": "toggleSplitOrientation",
      "keys": "ctrl+shift+l"
    },
    {
      "command": {
        "action": "resizePane",
        "direction": "right"
      },
      "keys": "alt+shift+l"
    },
    {
      "command": {
        "action": "nextTab"
      },
      "keys": "ctrl+shift+left"
    },
    {
      "command": {
        "action": "prevTab"
      },
      "keys": "ctrl+shift+right"
    }
  ],
  "alwaysShowNotificationIcon": true,
  "copyFormatting": "none",
  "copyOnSelect": false,
  "defaultProfile": "{61c54bbd-c2c6-5271-96e7-009a87ff44bf}",
